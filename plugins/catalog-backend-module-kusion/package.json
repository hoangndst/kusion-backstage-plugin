{
  "name": "@kusionstack/backstage-plugin-catalog-backend-module-kusion",
  "description": "The kusion backend module for the catalog plugin.",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "homepage": "https://kusionstack.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/KusionStack/kusion-backstage-plugin",
    "directory": "plugins/catalog-backend-module-kusion"
  },
  "license": "Apache-2.0",
  "private": true,
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "backstage": {
    "role": "backend-plugin-module",
    "pluginId": "catalog",
    "pluginPackage": "@backstage/plugin-catalog-backend"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack"
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "^1.0.2",
    "@kusionstack/kusion-api-client-sdk": "^1.1.5"
  },
  "devDependencies": {
    "@backstage/backend-test-utils": "^1.1.0",
    "@backstage/cli": "^0.29.0"
  },
  "files": [
    "dist",
    "config.d.ts"
  ],
  "configSchema": "config.d.ts"
}
